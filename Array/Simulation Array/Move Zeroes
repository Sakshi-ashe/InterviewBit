public class Solution {
    public int[] solve(int[] A) {
        
        int count = 0;
        
        for(int i=0; i<A.length; i++){
            if(A[i] != 0){
                A[count++] = A[i];
            }
        }
        
        while(count < A.length){
            A[count++] = 0;
        }
        
        return A;

    }
}
